---
import Editeur from "../../components/editeurs/Editeur.compact.astro";
import PageLayout from "../../components/layouts/PageLayout.astro";
import { getEditeurs, getString } from "../../helpers";

import Layout from "../../layouts/Layout.astro";
---

<Layout title={getString("editeurs")?.t}>
	<PageLayout sectionId="editeurs">
		{
			getEditeurs().map((editeur) => (
				<Editeur slot="page-content" id={editeur.id} />
			))
		}
	</PageLayout>
</Layout>
