---
import Editeur from "../../components/editeurs/Editeur.astro";
import { getEditeur, getEditeurs, getString } from "../../content";
import { getItemsStaticPaths } from "../../helpers";
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
	return getItemsStaticPaths(getEditeurs());
}

const { id } = Astro.params;
const editeur = getEditeur(id);
const { titre } = editeur;
---

<Layout title={titre}>
	<Editeur id={id} />
</Layout>
